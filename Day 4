#include <bits/stdc++.h>
using namespace std;

int main()
{
	int T;
	cin >> T;
	while(T--)
	{
	    int N, X;
	    cin >> N >> X;
	    int arr[N];
	    int current = X, max = X;
	    for(int i=0; i<N; i++)
	    {
	        cin >> arr[i];
	        current += arr[i];
	        if(current > max)
	        {
	            max = current;
	        }
	    }
	    cout << max << endl;
	}
}
