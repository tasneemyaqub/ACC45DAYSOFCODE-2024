#include <bits/stdc++.h>
using namespace std;

int main() {
    int T;
    cin >> T;
    while(T--)
    {
        int N;
        cin >> N;
        int arr[N];
        for(int i=0; i<N; i++)
        {
            cin >> arr[i];
        }
        sort(arr, arr+N);
        int current=1, max=1;
        for(int i=0; i<N-1; i++)
        {
            if(arr[i]==arr[i+1])
            {
                current++;
            }
            else
            {
                current=1;
            }
            if(current > max)
            {
                max = current;
            }
        }
        cout << N-max << endl;
    }
}
